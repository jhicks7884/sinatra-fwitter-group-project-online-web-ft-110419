<h2>Welcome, <%= @user.username %></h2>

<h3>All Tweets</h3>
<ul>
  <% Tweet.all.each do |tweet| %>
    <li>
      <%= tweet.content %>
    </li>

    <form action="/deliveries/<%=current_user.id%>/delete" method="post"> <br>  
     <input type="hidden" name="_method" value="delete" id='hidden'>
     <input type="submit" value="Delete deliveries">
    </form>

  <% end %>
</ul>
<a href="/logout">Log out</a>
